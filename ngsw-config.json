{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": [
        "/assets/**",
        "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
      ]
    }
  ],
  "dataGroups": [
    {
      "name": "music-cache",
      "urls": [
        "/**/*.mp3",
        "/assets/audio/**"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "7d",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ]
}